{
  "name": "@yarnpkg/fslib",
  "version": "2.4.0",
  "license": "MIT",
  "main": "./sources/index.ts",
  "sideEffects": false,
  "dependencies": {
    "@yarnpkg/libzip": "^2.2.1",
    "tslib": "^1.13.0"
  },
  "scripts": {
    "postpack": "rm -rf lib",
    "prepack": "run build:compile \"$(pwd)\"",
    "release": "yarn npm publish",
    "test:fslib": "run test:unit packages/yarnpkg-fslib",
    "test:watch:fslib": "run test:unit --watch packages/yarnpkg-fslib",
    "test": "pnpm run compile",
    "compile": "rimraf lib tsconfig.tsbuildinfo && tsc --build",
    "prepublishOnly": "pnpm run compile"
  },
  "publishConfig": {
    "main": "./lib/index.js",
    "typings": "./lib/index.d.ts"
  },
  "files": [
    "lib",
    "!*.map"
  ],
  "repository": "https://github.com/pnpm/pnpm/blob/master/packages/yarnpkg-fslib",
  "engines": {
    "node": ">=10.16"
  },
  "author": "Zoltan Kochan <z@kochan.io> (https://www.kochan.io/)",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "funding": "https://opencollective.com/pnpm",
  "homepage": "https://github.com/pnpm/pnpm/blob/master/packages/yarnpkg-fslib#readme"
}
